{
  "manifest_version": 2,
  "name": "Гошо навсякъде",
  "description": "Любимият Гошо навсякъде",
  "version": "0.1",
  "browser_action": {
    "default_icon": "img/icon128.png"
  },
  "content_scripts": [{
    "js": [
      "lib/jquery.min.js",
      "lib/lodash.min.js",
      "src/images.js",
      "src/interceptor.js"
    ],
    "matches": [
      "http://*/*",
      "https://*/*"
    ]
  }],
  "icons": {
    "128": "img/icon128.png",
    "16": "img/icon16.png"
  },
  "permissions": [
    "http://*/*",
    "https://*/*"
  ]
}
